# gitignore template for Drupal 9 projects
# earlier versions of Drupal are tracked in `community/PHP/`
/web/themes/contrib
/web/modules/contrib
/web/profiles/contrib
/web/libraries

# Ignore configuration files that may contain sensitive information
/web/sites/*/*settings*.php
/web/sites/*/*services*.yml
!/web/sites/default/settings.php

# Ignore paths that may contain user-generated content
/web/sites/*/files
/web/sites/*/public
/web/sites/*/private
/web/sites/*/files-public
/web/sites/*/files-private

# Ignore paths that may contain temporary files
/web/sites/*/translations
/web/sites/*/tmp
/web/sites/*/cache

# Resi composer install
/vendor
/web/core

# Ignore IDE and lando files
/.idea/
/.vscode/
*.iml
*.code-workspace
.lando.yml
adminer.php
/*.sql.gz

# Ignore all log files
*.log

# Ignore node modules
node_modules

# Ignore some css related stuff
*.css.map
*.css.map.map

# Ignore default text and exmple files
web/sites/*example*
web/CHANGELOG.txt
web/COPYRIGHT.txt
web/INSTALL*.txt
web/LICENSE.txt
web/MAINTAINERS.txt
web/UPGRADE.txt
web/README.txt
web/sites/README.txt
web/sites/all/modules/README.txt
web/sites/all/themes/README.txt

# Default files
!/web/sites/default/files/
/web/sites/default/files/*/*
/web/sites/default/files/*
web/sites/default/binary_www_old/*/*

# Ignore vendor dependencies and scripts
/vendor
/composer.phar
/composer
/robo.phar
/robo
/drush.phar
/drush
/drupal.phar
/drupal

### VisualStudioCode ###
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
!.vscode/*.code-snippets

# Local History for Visual Studio Code
.history/

# Built Visual Studio Code Extensions
*.vsix

### VisualStudioCode Patch ###
# Ignore all local history of files
.history
.ionide